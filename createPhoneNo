public class Kata {
  public static String createPhoneNumber(int[] numbers) {

    String name ="(";
      
      for(int i=0;i<numbers.length;i++){
       name +=numbers[i];
       
       if(i == 2){
       name += ") ";
       }
       
       else if(i == 5){
       name += "-";
       }
}

  return name;
  }
}
