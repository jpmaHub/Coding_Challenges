public class Printer {
    
    public static String printerError(String s) {
    int length = s.length();
    int len = 0;
   
    for(int i=0;i<length;i++){
      if(s.charAt(i) >'m'){
       len++;
        }
      }
      String res = String.valueOf(len) + "/" + String.valueOf(length);
      return res;
    }
    
    
    
}
